{
  "name": "ballot",
  "version": "0.0.0",
  "description": "Customizable anonymous online balloting system with ring signature authentication.",
  "repository": {
    "type": "git",
    "url": "http://github.com/b1f6c1c4/ballot"
  },
  "engines": {
    "npm": ">=5.5.1",
    "node": ">=8.9.3",
    "yarn": ">=1.3.2"
  },
  "author": "b1f6c1c4",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon graphiql.js",
    "start:prod": "yarn build && serve ./build",
    "start:apib": "aglio -i docs/public.apib -s",
    "start:cryptor": "aglio -i docs/cryptor.apib -s",
    "build": "graphdoc --force"
  },
  "graphdoc": {
    "schemaFile": "./docs/public.graphql",
    "output": "./build"
  },
  "dependencies": {
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "restify": "^6.3.4"
  }
}
